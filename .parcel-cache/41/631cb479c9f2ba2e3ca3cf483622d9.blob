var _ = require('../');
async function renderPosts() {
  const getPosts = await _.getDataFromServer('posts');
  const container = document.querySelector('.posts');
  let posts = '';
  getPosts.forEach(post => {
    const postHtml = `
      <div class="post">
        <h2>${post.title}</h2>
        <p>${post.body}</p>
      </div>`;
    posts += postHtml;
  });
  container.innerHTML = posts;
}
renderPosts();
