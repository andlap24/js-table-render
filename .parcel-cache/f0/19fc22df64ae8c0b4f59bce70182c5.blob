var _apiApi = require('./api/api');
async function renderPosts() {
  const getPosts = await _apiApi.getDataFromServer('posts');
  const container = document.querySelector('.container');
  let posts = '';
  getPosts.forEach(post => {
    const postHtml = `
      <div class="posts">
        <h2>${post.title}</h2>
        <p>${post.body}</p>
      </div>`;
    posts += postHtml;
  });
  container.innerHTML = posts;
}
renderPosts();
