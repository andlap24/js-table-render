* {
  box-sizing: border-box;
}
.posts {
  display: grid;
  grid-template-columns: repeat(2, 40vw);
  gap: 1.5vw;
  place-content: center;
}
.post {
  padding: 1vw;
  border-left: 10px solid purple;
  box-shadow: 2px 2px 2px black;
}
.post__title {
  font-size: 1.5vw;
  font-weight: 600;
  text-transform: uppercase;
  margin-bottom: 0.5vw;
}
