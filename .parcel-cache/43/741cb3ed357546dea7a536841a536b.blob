var _apiApi = require('./api/api');
async function renderPosts() {
  const posts = await _apiApi.getDataFromServer('posts').then(posts => posts);
  const postsHtml = `
    <div class="posts">
      <h2>${posts}</h2>
    </div>`;
  const container = document.querySelector('.container');
  container.innerHTML = postsHtml;
}
renderPosts();
