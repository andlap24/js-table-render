var _apiApi = require('./api/api');
async function renderPosts() {
  const posts = await _apiApi.getDataFromServer('posts');
  const postsHtml = `
    <div class="posts">
      <h2>${posts.title}</h2>
      <p>${posts.body}</p>
    </div>`;
  const container = document.querySelector('.container');
  container.innerHTML = postsHtml;
}
renderPosts();
